<config>
    <modules>
        <Webbhuset_Bugfix>
            <version>1.0.2</version>
        </Webbhuset_Bugfix>
    </modules>
    <global>
        <helpers>
            <whbugfix>
                <class>Webbhuset_Bugfix_Helper</class>
            </whbugfix>
        </helpers>
        <resources>
            <whbugfix_setup>
                <setup>
                    <module>Webbhuset_Bugfix</module>
                    <class>Mage_Core_Model_Resource_Setup</class>
                </setup>
            </whbugfix_setup>
        </resources>
        <sales>
            <quote>
                <totals>
                    <msrp>
                        <before>grand_total</before>
                    </msrp>
                    <shipping>
                        <after>subtotal,freeshipping,tax_subtotal,msrp</after>
                    </shipping>
                </totals>
            </quote>
        </sales>
    </global>
    <default>
        <catalog>
            <seo>
                <skip_invisible_product>1</skip_invisible_product>
                <skip_invisible_category>1</skip_invisible_category>
            </seo>
        </catalog>
    </default>
</config>
